<div class="container clearfix">

    <div class="title"><h4>Sign up</h4></div>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div><%= f.email_field :email, autofocus: true, placeholder: "Email" %></div>

      <div>
      <%= f.text_field :first_name, autocomplete: "off", placeholder: "First Name" %></div>

      <div>
      <%= f.text_field :last_name, autocomplete: "off", placeholder: "Last Name" %></div>

      <div><% if @validatable %><% end %>
        <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %></div>

      <div>
        <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Password Confirmation" %></div>

      <div><%= f.submit "Sign up" %></div>
    <% end %>



</div>
